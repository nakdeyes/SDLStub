#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/tim.rawcliffe/dev/git/SDLStub/build/mac
  make -f /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/tim.rawcliffe/dev/git/SDLStub/build/mac
  make -f /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/tim.rawcliffe/dev/git/SDLStub/build/mac
  make -f /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/tim.rawcliffe/dev/git/SDLStub/build/mac
  make -f /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/CMakeScripts/ReRunCMake.make
fi

