#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/source/3rdParty/SDL
  /Applications/CMake.app/Contents/bin/cmake -E cmake_symlink_library /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/Debug/libSDL3.0.dylib /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/Debug/libSDL3.0.dylib /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/Debug/libSDL3.dylib
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/source/3rdParty/SDL
  /Applications/CMake.app/Contents/bin/cmake -E cmake_symlink_library /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/Release/libSDL3.0.dylib /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/Release/libSDL3.0.dylib /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/Release/libSDL3.dylib
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/source/3rdParty/SDL
  /Applications/CMake.app/Contents/bin/cmake -E cmake_symlink_library /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/MinSizeRel/libSDL3.0.dylib /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/MinSizeRel/libSDL3.0.dylib /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/MinSizeRel/libSDL3.dylib
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/source/3rdParty/SDL
  /Applications/CMake.app/Contents/bin/cmake -E cmake_symlink_library /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/RelWithDebInfo/libSDL3.0.dylib /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/RelWithDebInfo/libSDL3.0.dylib /Users/tim.rawcliffe/dev/git/SDLStub/build/mac/RelWithDebInfo/libSDL3.dylib
fi

